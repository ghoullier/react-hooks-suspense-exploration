{
  "name": "react-hooks-suspense-exploration",
  "type": "static",
  "alias": "react-hooks-suspense-exploration.vincentriemer.com",
  "static": {
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=31536000"
          }
        ]
      }
    ]
  },
  "build": {
    "env": {
      "REACT_APP_TMDB_API_KEY": "@tmdb-api-key"
    }
  }
}
